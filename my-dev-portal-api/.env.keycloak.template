# Keycloak Configuration for Backend
AUTH_PROVIDER=Keycloak
KEYCLOAK_URL=https://your-keycloak-server.com
KEYCLOAK_REALM=your-realm-name
KEYCLOAK_CLIENT_ID=your-client-id

# Keycloak Admin API credentials (for user registration)
KEYCLOAK_ADMIN_USERNAME=admin
KEYCLOAK_ADMIN_PASSWORD=admin-password

# Other required environment variables (same as other providers)
MOESIF_APPLICATION_ID=your_moesif_application_id
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_ENDPOINT_SECRET=your_stripe_webhook_secret
